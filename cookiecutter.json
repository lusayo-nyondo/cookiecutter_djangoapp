{
    "project_name": "Django Webapp",
    "project_slug": "[@ cookiecutter.project_name.lower()|replace(' ', '_')|replace('-', '_')|replace('.', '_')|trim() @]",
    "project_description": "My Django Application",
    "project_domain": "[@ cookiecutter.project_slug @].pythonanywhere.com",
    "login_field_type": [
        "Username",
        "Email",
        "Both"
    ],
    "site_url": "/",
    "_jinja2_env_vars": {
        "block_start_string": "[%",
        "block_end_string": "%]",
        "variable_start_string": "[@",
        "variable_end_string": "@]"
    },
    "_copy_without_render": [
        "*.min.js"
    ]
}